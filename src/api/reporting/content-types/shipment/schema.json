{
  "kind": "collectionType",
  "collectionName": "shipments",
  "info": {
    "singularName": "shipment",
    "pluralName": "shipments",
    "displayName": "Reporting.Shipment",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "cargo": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::reporting.cargo",
      "mappedBy": "shipment"
    },
    "carrierId": {
      "type": "string"
    },
    "carbonOffsetPaid": {
      "type": "boolean"
    },
    "CO2TonsGenerated": {
      "type": "decimal"
    },
    "carbonOffsetCost": {
      "type": "decimal"
    },
    "notes": {
      "type": "text"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "Regular Route",
        "Ad Hoc",
        "Aid Swap/Local Transfer",
        "Other"
      ]
    },
    "DARoles": {
      "type": "customField",
      "options": [
        "Needs - Needs Assessment",
        "Aid Sourcing - In-Kind Donations",
        "Aid Sourcing - Procurement",
        "Aid Sourcing - Community Collection",
        "Needs - Aid Matching",
        "First Mile - Transportation",
        "First Mile - Storage - Community",
        "First Mile - Storage - Commercial",
        "Main Leg - Transportation",
        "Customs - Transit",
        "Customs - Export",
        "Customs - Import",
        "Last Mile - Transportation",
        "Last Mile - Storage - Community",
        "Last Mile - Storage - Commercial",
        "Other"
      ],
      "customField": "plugin::multi-select.multi-select"
    },
    "sendingCountry": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::geo.country"
    },
    "receivingCountry": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::geo.country"
    },
    "number": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "project": {
      "type": "enumeration",
      "enum": [
        "Covid 19",
        "Disaster Relief",
        "Moria Fire",
        "Refugee Aid - Europe",
        "Refugee Aid - Lebanon",
        "Social Enterprise Support",
        "Ukraine",
        "US ARR",
        "Other"
      ]
    },
    "importer": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::group.group"
    },
    "exporter": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::group.group"
    }
  }
}
