{
  "kind": "collectionType",
  "collectionName": "cargos",
  "info": {
    "singularName": "cargo",
    "pluralName": "cargos",
    "displayName": "Reporting.Cargo",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "packageCount": {
      "type": "integer"
    },
    "packageUnit": {
      "type": "enumeration",
      "enum": [
        "Bag",
        "Medium Bag",
        "Large Bag",
        "Box",
        "Banana Box",
        "Pallet",
        "Euro Pallet",
        "Item",
        "Single Item"
      ]
    },
    "used": {
      "type": "boolean"
    },
    "itemCount": {
      "type": "integer"
    },
    "valueOverride": {
      "type": "boolean"
    },
    "valueOverrideCurrency": {
      "type": "enumeration",
      "enum": [
        "USD",
        "GBP",
        "EUR",
        "LBP",
        "LTL",
        "RSD",
        "BAM"
      ]
    },
    "normalizedValuePerItem": {
      "type": "decimal"
    },
    "totalNormalizedValue": {
      "type": "decimal"
    },
    "valueInSendingCountry": {
      "type": "decimal"
    },
    "valueInReceivingCountry": {
      "type": "decimal"
    },
    "totalNeedsMet": {
      "type": "decimal"
    },
    "standardItemCount": {
      "type": "integer"
    },
    "shipment": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::reporting.shipment",
      "inversedBy": "cargo"
    },
    "item": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::product.item"
    },
    "sendingCountry": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::geo.country"
    },
    "receivingCountry": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::geo.country"
    },
    "countryGDPContextCostOverride": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::geo.country"
    },
    "sender": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::group.group"
    },
    "receiver": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::group.group"
    }
  }
}
