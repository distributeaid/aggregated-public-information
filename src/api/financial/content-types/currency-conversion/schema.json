{
  "kind": "collectionType",
  "collectionName": "currency_conversions",
  "info": {
    "singularName": "currency-conversion",
    "pluralName": "currency-conversions",
    "displayName": "Financial.CurrencyConversions",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "year": {
      "type": "string",
      "regex": "^2019|202[0-9]|203[0-9]$",
      "required": true
    },
    "month": {
      "type": "integer",
      "required": true,
      "max": 12,
      "min": 1
    },
    "currency": {
      "type": "enumeration",
      "enum": ["USD", "GBP", "EUR", "LBP", "LTL", "RSD", "BAM"],
      "required": true
    },
    "equivalentToUSD": {
      "type": "float",
      "required": true
    },
    "source": {
      "type": "string"
    },
    "notes": {
      "type": "text"
    }
  }
}
