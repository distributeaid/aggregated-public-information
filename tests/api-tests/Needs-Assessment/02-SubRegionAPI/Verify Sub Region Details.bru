meta {
  name: Verify Sub Region Details
  type: http
  seq: 2
}

get {
  url: {{base_url}}/api/subregions/{{sub_region_document_id}}
  body: json
  auth: none
}

tests {
  test("Status code is 200 or 201", function () {
      expect(res.getStatus()).to.be.oneOf([200, 201]);
  });
  
  test("Correct information saved", function () {
      var jsonData = res.getBody();
      expect(jsonData.data.name).to.eql('DemoSubregionNew');
      expect(jsonData.data.overview).to.eql('This is SubRegion');
  });
  
}
