meta {
  name: Update Region
  type: http
  seq: 3
}

put {
  url: {{base_url}}/api/regions/{{region_document_id}}
  body: json
  auth: none
}

body:json {
  {
    "data": {
      "governmentResponse": "This is government response"
    }
  }
}

tests {
  test("Status code is 200 or 201", function () {
      expect(res.getStatus()).to.be.oneOf([200, 201]);
  });
  
  test("Correct information updated", function () {
      var jsonData = res.getBody();
      expect(jsonData.data.name).to.eql('DemoRegionNew');
      expect(jsonData.data.overview).to.eql('This is Region');
     expect(jsonData.data.governmentResponse).to.eql('This is government response');
  });
  
  
  
}
