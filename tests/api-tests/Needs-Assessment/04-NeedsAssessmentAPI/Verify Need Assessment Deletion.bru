meta {
  name: Verify Need Assessment Deletion
  type: http
  seq: 8
}

get {
  url: {{base_url}}/api/needs/{{needsAssessment_document_id}}
  body: json
  auth: none
}

tests {
  test("Status code is 404", function () {
      expect(res.getStatus()).to.be.oneOf([400, 404]);
  });
  
  test("Deleted Need Assessment not valid anymore", function () {
      var jsonData = res.getBody();
      expect(jsonData.error.name).to.eql('NotFoundError');
  });
  
  
}
